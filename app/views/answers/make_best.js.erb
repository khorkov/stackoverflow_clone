$('#answers').html('<%= j render "answers" %>');
$(function() {
  return $('.edit-answer-link').click(function(e) {
    var answer_id;
    e.preventDefault();
    $(this).hide();
    answer_id = $(this).data('answerId');
    $('.content-edit-answer-' + answer_id).hide();
    return $('form#form-edit-answer-' + answer_id).show();
  });
});
